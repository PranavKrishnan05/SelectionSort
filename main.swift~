var i = unsortedText
print("Pass: \(0), Swaps: \(0)/\(0), Array: \(i)")
var totswaps = 0

for x in 1..<i.count {
    var swaps = 0
    var j = x
    let key = i[j]
    /*counter() */    
    /*  randnum() */
    while j > 0 && key < i[j - 1] {
        i[j] = i[j - 1]
        swaps += 1
        j -= 1
    }
    totswaps += swaps
    i[j] = key
    print("Pass: \(x), Swaps: \(swaps)/\(totswaps), Array: \(i)")


}
    
